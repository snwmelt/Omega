        xmlns:local="clr-namespace:Omega"
        mc:Ignorable="d"
        Title="New Entry" Height="180" Width="298.667" Foreground="{x:Null}">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="55*"/>
            <ColumnDefinition Width="42*"/>
        </Grid.ColumnDefinitions>
        <TextBox x:Name="textBox" Height="24" Margin="-46.333,39.667,32,0" TextWrapping="Wrap" VerticalAlignment="Top" HorizontalAlignment="Right" Width="140" d:LayoutOverrides="VerticalAlignment" Grid.Column="1"/>
