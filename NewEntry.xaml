        <Label x:Name="label" Content="Start Amount :" Height="24" Margin="10,0,57.333,45.334" VerticalAlignment="Bottom"/>
        <Label x:Name="nameAccount" Content="Name :" HorizontalAlignment="Left" Height="24" Margin="10,39.667,0,0" VerticalAlignment="Top" Width="53.667"/>

    </Grid>
</Window>
